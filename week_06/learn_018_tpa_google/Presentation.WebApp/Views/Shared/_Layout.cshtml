<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@(string.IsNullOrEmpty(ViewData["Title"] as String) ? "Third-Party Authentication" : @ViewData["Title"] + " - Third-Party Authentication")</title>
    <script type="importmap"></script>
    @* Bootstrap CDN *@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    @* Bootstrap Icons CDN *@
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css">
    @* Fontawesome Icons *@
    <link rel="stylesheet" href="https://kit.fontawesome.com/a576612222.css" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
</head>
<body>

<div class="wrapper-portal">
    <div class="logotype">
        <partial name="_VideoLink"/>
    </div>
    <div class="side">
        <partial name="_HeaderPartial"/>
    </div>
    <div class="top">
        @* <a class="btn btn-gray" asp-controller="Auth" asp-action="SignOut">Logout</a> *@
        <div class="d-flex">
        </div>
    </div>

    <main class="main">
        @RenderBody() 
    </main>
</div>

@* Bootstrap Javascript CDN *@
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>