<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - Presentation.WebApp_Injected</title>
    <script type="importmap"></script>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true"/>
    <link rel="stylesheet" href="~/Presentation.WebApp_Injected.styles.css" asp-append-version="true"/>
</head>
<body>

<div class="wrapper">
    <header></header>
    <main>
        @RenderBody()
    </main>
    <footer></footer>
</div>


<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>